  5.        Create an INVOICE database and having following tables.

                      Customer(id, name, address, city, ph_no)
                      Product(pid,  pname, price (default 0))
                      Invoice_master(inv_id, cust_id(FK), inv_date)
                      Invoice_item(inv_id (FK),  pid(FK), quantity)


 create database INVOICE;
  use INVOICE;

//table customer
  create table Customer(id int primary key,name varchar(20),address  varchar(100),city varchar(20),ph_no int);

desc Customer;
+---------+--------------+------+-----+---------+-------+
| Field   | Type         | Null | Key | Default | Extra |
+---------+--------------+------+-----+---------+-------+
| id      | int(11)      | NO   | PRI | NULL    |       |
| name    | varchar(20)  | YES  |     | NULL    |       |
| address | varchar(100) | YES  |     | NULL    |       |
| city    | varchar(20)  | YES  |     | NULL    |       |
| ph_no   | int(11)      | YES  |     | NULL    |       |
+---------+--------------+------+-----+---------+-------+


//table product
 create table Product(pid int primary key,pname varchar(50),price int DEFAULT 0);

desc Product;
+-------+-------------+------+-----+---------+-------+
| Field | Type        | Null | Key | Default | Extra |
+-------+-------------+------+-----+---------+-------+
| pid   | int(11)     | NO   | PRI | NULL    |       |
| pname | varchar(50) | YES  |     | NULL    |       |
| price | int(11)     | YES  |     | 0       |       |
+-------+-------------+------+-----+---------+-------+


//table Invoice_master
create table Invoice_master(
inv_id int primary key, 
cust_id int,
inv_date date,
foreign key(cust_id) references Customer(id));

desc Invoice_master;
+----------+---------+------+-----+---------+-------+
| Field    | Type    | Null | Key | Default | Extra |
+----------+---------+------+-----+---------+-------+
| inv_id   | int(11) | NO   | PRI | NULL    |       |
| cust_id  | int(11) | YES  | MUL | NULL    |       |
| inv_date | date    | YES  |     | NULL    |       |
+----------+---------+------+-----+---------+-------+



//table Invoice_item
 create table Invoice_item( inv_id int,pid int,
quantity int,
foreign key(inv_id) references Invoice_master(inv_id),
foreign key(pid) references Product(pid));

desc Invoice_item;
+----------+---------+------+-----+---------+-------+
| Field    | Type    | Null | Key | Default | Extra |
+----------+---------+------+-----+---------+-------+
| inv_id   | int(11) | YES  | MUL | NULL    |       |
| pid      | int(11) | YES  | MUL | NULL    |       |
| quantity | int(11) | YES  |     | NULL    |       |
+----------+---------+------+-----+---------+-------+

